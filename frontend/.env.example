# Environment Variables Example
# Copy this file to .env.local for local development overrides

# API Configuration
# Development: http://localhost:5050/api/v1
# Production: https://api.jaydenmetz.com/api/v1
REACT_APP_API_URL=https://api.jaydenmetz.com/api/v1

# WebSocket Configuration
# Development: ws://localhost:5050
# Production: wss://api.jaydenmetz.com
REACT_APP_WS_URL=wss://api.jaydenmetz.com

# React Fast Refresh
FAST_REFRESH=false