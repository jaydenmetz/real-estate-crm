# Backend Environment Variables Template
# Copy this file to .env and fill in your values
# NEVER commit .env files with actual values

# Server Configuration
NODE_ENV=development
PORT=5050
API_VERSION=v1

# Database (Use Railway or local PostgreSQL)
DATABASE_URL=postgresql://username:password@host:port/database
REDIS_URL=redis://localhost:6379

# Security - Generate secure random strings
# Generate with: openssl rand -hex 32
JWT_SECRET=
JWT_EXPIRE=30d

# API Keys - Generate unique keys for each service
# Generate with: openssl rand -hex 32
MASTER_API_KEY=
EXECUTIVE_API_KEY=
BUYER_MANAGER_KEY=
LISTING_MANAGER_KEY=
OPS_MANAGER_KEY=

# AI Agent API Keys (for v2)
BUYER_QUALIFIER_KEY=
BUYER_NURTURE_KEY=
SHOWING_COORD_KEY=
LISTING_LAUNCH_KEY=
MARKET_ANALYST_KEY=
LISTING_MARKETING_KEY=
TRANSACTION_COORD_KEY=
COMPLIANCE_KEY=
FINANCIAL_ANALYST_KEY=
DATABASE_SPECIALIST_KEY=

# Twilio (Optional - for SMS features)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=

# AWS S3 (Optional - for document storage)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_BUCKET_NAME=
AWS_REGION=us-west-2

# AI Services (for v2)
ANTHROPIC_API_KEY=
OPENAI_API_KEY=

# Frontend URL
FRONTEND_URL=http://localhost:3000

# Monitoring (Optional but recommended)
SENTRY_DSN=
DATADOG_API_KEY=

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100