{
  "name": "real-estate-crm-api",
  "version": "1.0.0",
  "description": "Real Estate CRM API with AI Agent Integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:local": "cp .env.local .env && nodemon server.js",
    "dev:prod": "cp .env.production .env && NODE_ENV=production node server.js",
    "migrate": "node src/scripts/migrate.js",
    "migrate:create": "node src/scripts/create-migration.js",
    "test": "jest",
    "test:errors": "node src/test-error-logging.js",
    "env:local": "./scripts/switch-env.sh local",
    "env:prod": "./scripts/switch-env.sh production"
  },
  "dependencies": {
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^10.15.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.5.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "dotenv-vault": "^1.27.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-cron": "^3.0.3",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "rate-limit-redis": "^4.2.2",
    "redis": "^4.6.10",
    "sharp": "^0.34.3",
    "socket.io": "^4.8.1",
    "twilio": "^4.23.0",
    "uuid": "^9.0.1",
    "validator": "^13.15.15",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  }
}
