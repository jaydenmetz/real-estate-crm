{
  "name": "real-estate-crm-api",
  "version": "1.0.0",
  "description": "Real Estate CRM API with AI Agent Integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:local": "cp .env.local .env && nodemon server.js",
    "dev:prod": "cp .env.production .env && NODE_ENV=production node server.js",
    "migrate": "node src/scripts/migrate.js",
    "migrate:create": "node src/scripts/create-migration.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:unit": "jest --testPathPattern='.*(test|spec)\\.js$' --testPathIgnorePatterns='integration'",
    "test:integration": "jest --testPathPattern='integration'",
    "test:errors": "node src/test-error-logging.js",
    "env:local": "./scripts/switch-env.sh local",
    "env:prod": "./scripts/switch-env.sh production",
    "lint": "eslint src --max-warnings=0",
    "lint:fix": "eslint src --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^10.26.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "dotenv-vault": "^1.27.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "google-auth-library": "^10.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "node-cron": "^4.2.1",
    "openai": "^6.9.1",
    "pdf-parse": "^2.4.5",
    "pg": "^8.11.3",
    "rate-limit-redis": "^4.3.0",
    "redis": "^5.10.0",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.10.6",
    "uuid": "^13.0.0",
    "validator": "^13.15.15",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@jest/globals": "^30.2.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-extended": "^6.0.0",
    "jest-mock-extended": "^3.0.5",
    "lint-staged": "^16.2.3",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "jest --bail --findRelatedTests --passWithNoTests"
    ]
  }
}
